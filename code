local Held = false
local gcheck = true
local speed = 1 
local player = game.Players.LocalPlayer

function checker()
    if Held == true and player.Character.Humanoid.MoveDirection ~= Vector3.new(0,0,0) then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-speed)
    end
end

local stepped = game:GetService('RunService').Stepped
local matched;

local connection = stepped
game.Players.LocalPlayer.Chatted:Connect(function(msg)
    if string.match(string.lower(msg), "^s [%d]+$") then
       matched = string.match(msg, "[%d]+")
       speed = type(matched) == "number" and matched or speed
    end
end)

game:GetService("UserInputService").InputBegan:connect(function(inputObject, gameProcessedEvent)
    if inputObject.KeyCode == Enum.KeyCode.LeftControl and not gameProcessedEvent and gcheck then
        Held = true
        connection = connection:Connect(checker)
    elseif inputObject.KeyCode == Enum.KeyCode.K and not gameProcessedEvent then        
        gcheck = not gcheck
    end
end)

game:GetService("UserInputService").InputEnded:connect(function(inputObject,gameProcessed)
    if inputObject.KeyCode == Enum.KeyCode.LeftControl and not gameProcessed then
        Held = false
        connection:Disconnect()
        connection = stepped
    end
end)

warn("Executed")
